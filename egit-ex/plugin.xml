<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         point="org.eclipse.ui.actionSets">
      <actionSet
            label="Git Action Set"
            visible="true"
            id="egit-ex.actionSet">
         <menu
               label="git-ex"
               id="gitMenu">
            <separator
                  name="gitGroup">
            </separator>
         </menu>
         <action
               class="egitex.actions.SvnRebaseAction"
               icon="icons/sample.gif"
               id="egitex.actions.SvnRebaseAction"
               label="pull from svn"
               menubarPath="gitMenu/gitGroup"
               toolbarPath="gitGroup"
               tooltip="pull from svn">
         </action>
         <action
               label="push to svn"
               icon="icons/sample.gif"
               class="egitex.actions.SvnDcommitAction"
               tooltip="push to svn"
               menubarPath="gitMenu/gitGroup"
               toolbarPath="gitGroup"
               id="egitex.actions.SvnDcommitAction">
         </action>
      </actionSet>
   </extension>

</plugin>
